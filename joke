import requests

# API URL
url = "https://official-joke-api.appspot.com/random_joke"

# Request bhejna
response = requests.get(url)

print("\n--- Random Joke ---")
if response.status_code == 200:
    joke = response.json()
    print(joke["setup"])       # Joke ka pehla part
    print(joke["punchline"])   # Joke ka answer
else:
    print("Error:", response.status_code)
